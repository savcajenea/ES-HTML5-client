<root>
	<EasySite>
                <proxyUrl>http://easyting.dk/easyscreen/proxy?file_url=</proxyUrl>
                <module>http://easyting.dk/profiles/ding2/modules/ding_easyscreen_d7/easyscreen_core/swf/EasySite_mod.swf</module>

                <hash>123</hash>
